{
  "name": "uganda-tech-connect",
  "version": "1.0.0",
  "description": "Uganda Tech Connect - Innovation and Technology Platform",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "serve": "python manage.py runserver"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uganda-tech-connect/platform.git"
  },
  "keywords": [
    "typescript",
    "django",
    "tailwind",
    "innovation",
    "technology",
    "uganda"
  ],
  "author": "Uganda Tech Connect",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
